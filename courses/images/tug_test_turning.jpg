<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 200" width="300" height="200">
  <rect width="300" height="200" fill="#f3f4f6"/>
  <rect x="10" y="10" width="280" height="180" fill="white" stroke="#d1d5db" stroke-width="2" rx="5"/>
  
  <!-- Chair -->
  <rect x="40" y="120" width="50" height="60" fill="#8b5cf6" stroke="#7c3aed" stroke-width="2"/>
  <rect x="35" y="115" width="60" height="10" fill="#8b5cf6" stroke="#7c3aed" stroke-width="2"/>
  
  <!-- Person turning at cone -->
  <circle cx="220" cy="105" r="10" fill="#fbbf24"/>
  <rect x="210" y="105" width="20" height="30" fill="#3b82f6"/>
  
  <!-- Turning motion arrow -->
  <path d="M 235 115 Q 245 105 245 125 Q 245 135 235 135" stroke="#10b981" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- 3-meter line -->
  <line x1="100" y1="150" x2="230" y2="150" stroke="#ef4444" stroke-width="3" stroke-dasharray="5,5"/>
  <text x="165" y="145" font-family="Arial, sans-serif" font-size="12" fill="#ef4444" text-anchor="middle">3 meters</text>
  
  <!-- Cone -->
  <polygon points="230,140 225,160 235,160" fill="#f59e0b" stroke="#d97706" stroke-width="1"/>
  
  <text x="150" y="30" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle">TUG Test Setup</text>
  <text x="150" y="50" font-family="Arial, sans-serif" font-size="12" text-anchor="middle">Phase 3: Turning</text>
  
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="10" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#10b981"/>
    </marker>
  </defs>
</svg>